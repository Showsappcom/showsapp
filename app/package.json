  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "build-mobile": "cross-env NODE_ENV=mobile_production webpack --display-chunks",
    "build:prod": "cross-env NODE_ENV=aot_production DEV=false webpack --display-chunks --progress",
    "build-visual:prod": "cross-env NODE_ENV=aot_production_vis DEV=false webpack --display-chunks --progress",
    "build-cordova:prod": "cross-env NODE_ENV=aot_cordova DEV=false webpack --display-chunks --progress",
    "build-wathc:prod-watch": "cross-env NODE_ENV=aot_production DEV=false webpack --display-chunks --progress --watch",
    "build:prod-dev": "cross-env NODE_ENV=aot_production DEV=true webpack --display-chunks --progress",
    "build-mobile:prod": "cross-env NODE_ENV=aot_mobile_production DEV=false webpack --display-chunks --progress",
    "build-mobile:prod-dev": "cross-env NODE_ENV=aot_mobile_production DEV=true webpack  --display-chunks --progress",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli target",
    "compodocDesktop": "./node_modules/.bin/compodoc -p tsconfig.json -d ./documentation/desktop",
    "compodocMobile": "./node_modules/.bin/compodoc -p tsconfigMobile.json -d ./documentation/mobile",
    "lint": "run-s lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js src server",
    "lint:ts": "tslint 'src/**/*.ts'",
    "prebuild": "npm run clean",
    "pretest": "npm run lint -s",
    "karma:test": "cross-env NODE_ENV=test karma start --single-run",
    "pre-prod-build": "gulp prod_build",
    "pre-dev-build": "gulp dev_build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "server-mobile:dev": "cross-env NODE_ENV=mobile_development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
